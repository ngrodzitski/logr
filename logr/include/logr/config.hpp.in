// clang-format off
#pragma once

#define LOGR_VERSION_CODE( major, minor, patch ) \
    ( ( ( major ) << 16UL ) + ( ( minor ) << 8UL ) + ( ( patch ) << 0UL ))

#define LOGR_VERSION_MAJOR @TARGET_LIB_VERSION_MAJOR@UL
#define LOGR_VERSION_MINOR @TARGET_LIB_VERSION_MINOR@UL
#define LOGR_VERSION_PATCH @TARGET_LIB_VERSION_PATCH@UL

#define LOGR_VERSION \
    LOGR_VERSION_CODE( LOGR_VERSION_MAJOR, LOGR_VERSION_MINOR, LOGR_VERSION_PATCH )

